{
  "name": "fleti",
  "description": "An attempt at making filter/map/reduce faster via let i for loops",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/fleti.umd.cjs",
  "module": "./dist/fleti.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/fleti.js",
      "require": "./dist/fleti.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "dirty-build": "vite build",
    "build": "tsc && $npm_execpath run dirty-build",
    "build-test": "bash tests/build/start.sh && $npm_execpath run dirty-build && bash tests/build/end.sh",
    "test": "$npm_execpath run build-test && $npm_execpath run pw",
    "pw": "playwright test --browser=all"
  },
  "devDependencies": {
    "@playwright/test": "^1.32.2",
    "@types/node": "^18.15.11",
    "prettier": "^2.8.7",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vite-plugin-dts": "^2.2.0"
  }
}
